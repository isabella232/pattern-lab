<header class="c-main-header">
  <div class="o-ad o-ad--leaderboard">
    {%
      include '@atoms/images/image.twig'
      with {
        "img": {
          "src": "https://placehold.it/728x90"
        }
      }
    %}
  </div>
  <div class="c-main-header__inner js-stop-prop">
    <button class="c-menu-toggle js-toggle-parent" id="menu-toggle" aria-expanded="false">
      <span class="is-vishidden">Menu</span>
      <span class="c-menu-toggle__line"></span>
    </button>

    {% include '@organisms/global/side-menu.twig' %}

    <div class="c-main-header__branding">
      {% include '@atoms/images/gothamist-logo.twig' %}
    </div>
    {%
      include '@molecules/navigation/primary.twig'
      with {
        "primary_nav_classes": "c-primary-nav--top"
      }
    %}
    <div class="c-main-header__right">
      {%
        include '@atoms/buttons/button.twig'
        with {
          "button_text": "Donate",
          "button_classes": "c-main-header__donate o-button--sm"
        }
      %}
      <button class="c-search-toggle js-toggle" id="search-toggle" aria-expanded="false">
        <span class="c-search-toggle__icon o-icon o-icon--search u-icon--xs">{% include '@atoms/icons/icon-search.twig' %}</span>
      </button>
    </div>
  </div>
</header>
